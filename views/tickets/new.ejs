<%- include('../partials/header') %>
  <h1>Add A New Ticket</h1><br>
    <%- include('../partials/nav') %>

<form id="add-ticket-form" action="/tickets" method="POST">
  <label>Seat:</label>
  <input name="seat" required pattern="[A-F][1-9]\d?">
  <label>Price:</label>
  <input type="number" name="price">
  <label>Flight:</label>
  <input type="ObjectId" name="flight">
  <input type="submit" value="Add Ticket">
</form>





